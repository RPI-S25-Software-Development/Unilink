{
  "expo": {
    "name": "Unilink",
    "slug": "Unilink",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Unilink_Logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Unilink_Logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.Unilink"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/Unilink_Logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/Unilink_Logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "06f843bb-bf2f-452d-928f-6db6bc640ec3"
      }
    },
    "platforms": ["ios", "android", "web"]
  }
}
