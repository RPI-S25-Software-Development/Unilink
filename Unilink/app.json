{
  "expo": {
    "name": "Unilink",
    "slug": "unilink",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Unilink_Logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Unilink_Logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.Unilink"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/Unilink_Logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/Unilink_Logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "8bd9fb82-6801-437b-becd-03e401677447"
      }
    },
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "owner": "bajajb"
  }
}
